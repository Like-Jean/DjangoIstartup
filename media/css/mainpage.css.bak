body{
background-color:snow;
font-family:SimSun;
margin:0px;
font-family:SimHei;
}
.title{
width:100%;
height:140px;
background-color:#11aaff;

}
.tar{
width:100%;
height:33px;
color:gray;
font-weight:bold;
background-image:url("../img/bg.jpg");
border-bottom:solid 1px #88aaff;
}
#logo{
width:24px;
height:24px;
margin-left:8px;

}
#com{
padding-top:0;
padding-bottom:8px;
width:120px;
font-size:28px;
background-image:url("../img/bg.jpg");
padding-right:10px;
opacity:1;
}
#com:hover{
background-image:url("../img/com.jpg");
}
.tar ul{
list-style-type:none;
margin:0px;
float:left;
}
.tar li{
text-align:center;
padding-top:8px;
background-image:url("../img/bt.jpg");
opacity:0.8;
margin-left:1px;
width:80px;
height:25px;
font-style:bold;
color:#44444;
border-radius:3px;
}
.tar li:hover{
background-image:url("../img/bt2.jpg");
}
.left li{
float:left;
}
.right{
margin-right:10px;
float:right;
}
.right li{
float:right;
}
.brief{
clear:both;
width:100%;
background-image:url("../img/gz.jpg");
text-align:center;
padding-top:40px;
padding-bottom:140px;
border-bottom:solid 1px #88aaff;
height:200px;
}
#logo1{
width:100px;
height:100px;
}
.brief h1{
font-size:50px;
font-style:bold;
color:#ffffff;
}

.brief p{
color:white;
background-color:#0000ff;
}
.brief a{
border:solid 1px #ffffff;
color:#eeeeff;
line-height:36px;
padding:5px 40px;
font-size:16px;
text-decoration:none;
font-weight:bold;
border-radius:4px;
background-color:#3366aa;
opacity:0.9;
}
.brief a:hover{
background-color:#2266ff;
border:solid 0;
color:#ffffff;
opacity:1;
}
.content{
text-align:center;
margin-top:260px;
width:100%;
}
#label_starter{
font-size:30px;
font-weight:bold;
color:#888888;
height:60px;
}
.content ul{
list-style-type:none;
text-align:center;
}

.starter{
background-color:#00aaff;
width:100%;
padding:10%;
padding-top:0;
padding-bottom:0;
}
.img_starter{
width:100px;
height:100px;
border-radius:50px;
border:solid 4px #ffffff;
}
.info_starter{
width:16%;
height:220px;
float:left;
margin-left:8%;
margin-top:10px;
padding-top:10px;
color:#555555;
border-radius:5px;
font-size:20px;
}
.info_starter:hover{
background-color:#00bbee;
color:white;
}
.name_starter{
}
.title_invest{
clear:both;
}
.fooster{
width:100%;
height:160px;
margin-top:260px;
background:#444444;
text-align:center;
}
.app_img{
margin-top:10px;
margin-left:40px;
margin-right:40px;
}
.label{
color:#aaaaaa;
}

